{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build/client",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/server"
    }
  ],
  "functions": {
    "api/server.js": {
      "runtime": "nodejs20.x",
      "includeFiles": "build/**"
    }
  }
}
